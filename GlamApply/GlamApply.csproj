<Project Sdk="Dalamud.NET.Sdk/14.0.1">
    <!-- Assembly Configuration -->
    <PropertyGroup>
        <AssemblyName>GlamApply</AssemblyName>
        <AssemblyVersion>0.1.0.3</AssemblyVersion>
        <FileVersion>0.1.0.3</FileVersion>
        <InformationalVersion>0.1.0.3</InformationalVersion>
    </PropertyGroup>

    <!-- Build Configuration -->
    <PropertyGroup>
        <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
        <Nullable>enable</Nullable>
        <WarningsAsErrors>Nullable</WarningsAsErrors>
    </PropertyGroup>

    <!-- Included Content -->
    <ItemGroup>
        <Content Include="GlamApply.json" CopyToOutputDirectory="Always" />
    </ItemGroup>
</Project>
